<h1>Solar Device Form</h1>
<form method="POST" enctype="multipart/form-data" action="/solar_device/add" />
  User: <input type="text" name="sender" /> <br/>
  firstName: <input type="text" name="firstName" /> <br/>
  lastName: <input type="text" name="lastName" /> <br/>
  date_of_installation: <input type="date" name="date_of_installation" /> <br/>
  address: <input type="text" name="address" /> <br/>
  City: <input type="text" name="city" /> <br/>
  State: <input type="text" name="state" /> <br/>
  Zipcode: <input type="text" name="zipcode" /> <br/>
  Country: <input type="text" name="country" /> <br/>
  Nameplate: <input type="text" name="nameplate" /> <br/>
  public_key: <input type="text" name="public_key" /> <br/> 
  <!-- file: <input name='proof' type="file" /> <br /> -->
  file: <input type="text" name='proof' /> <br />
  type: <input type="text" name="file_type" />
  <input type="submit" value="submit">
</form>